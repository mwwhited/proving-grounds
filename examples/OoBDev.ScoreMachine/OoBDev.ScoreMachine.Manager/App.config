<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>

  <appSettings>
    <add key="ScoreMachine.Model" value="SG" />
    <!--
    <add key="ScoreMachine.Model" value="Emulator"/>
    <add key="ScoreMachine.Model" value="SG"/>
    <add key="ScoreMachine.Model" value="Favero"/>
    -->
    <add key="ScoreMachine.Serial.PortName" value="COM4" />
    <add key="ScoreMachine.Serial.BaudRate" value="9600" />
    <add key="Owin.Uri" value="http://*:5001/" />
    <add key="Owin.BasePath" value="../../www" />
    <add key="NeTV.IP" value="10.0.88.1" />
    <add key="NeTV.Target" value="http://10.0.88.4:5001/" />
  </appSettings>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.0.0" newVersion="4.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin.Security" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.0.0" newVersion="4.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Cors" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.2.7.0" newVersion="5.2.7.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>

  <!--
        public static TraceSource DataHub { get; } = new TraceSource("OoBDev.DataHub", SourceLevels.Information);
        public static TraceSource OwinRequest { get; } = new TraceSource("OoBDev.Owin.Request", SourceLevels.Information);
        public static TraceSource NeTV { get; } = new TraceSource("OoBDev.NeTV", SourceLevels.Information);
        public static TraceSource ScoreMachine { get; } = new TraceSource("OoBDev.ScoreMachine", SourceLevels.Information);
        -->
  <system.diagnostics>
    <sources>
      <source name="OoBDev.DataHub" switchName="sourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="console" />
          <add name="errorLog" />
        </listeners>
      </source>
      <source name="OoBDev.Owin.Request" switchName="sourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="console" />
          <add name="errorLog" />
        </listeners>
      </source>
      <source name="OoBDev.NeTV" switchName="sourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="console" />
          <add name="errorLog" />
        </listeners>
      </source>
      <source name="OoBDev.ScoreMachine" switchName="sourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="console" />
          <add name="errorLog" />
        </listeners>
      </source>

    </sources>
    <switches>
      <add name="sourceSwitch" value="Information,Warning,Verbose" />
    </switches>
    <trace autoflush="true" />
    <sharedListeners>
      <add name="console" type="System.Diagnostics.ConsoleTraceListener">
        <filter type="System.Diagnostics.EventTypeFilter" initializeData="Information,Error,Warning,Verbose" />
      </add>
      <add name="errorLog" type="System.Diagnostics.TextWriterTraceListener" initializeData="myListener.log">
        <filter type="System.Diagnostics.EventTypeFilter" initializeData="Error,Information" />
      </add>
    </sharedListeners>
  </system.diagnostics>

</configuration>
