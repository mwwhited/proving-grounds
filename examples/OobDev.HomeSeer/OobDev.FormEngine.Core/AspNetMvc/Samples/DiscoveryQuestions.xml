<?xml version="1.0" encoding="utf-8" ?>
<form xmlns="http://schemas.outofbanddevelopment.com/FormEngine/2015/05/v1"
        title="Discovery Questions">

  <page slug="CustomerInfo" title="Customer Info" entry-point="true">
    <button label="Back" class="glyphicon glyphicon-menu-left" action="goto" target="Implementation" />
    <button label="Next" class="glyphicon glyphicon-menu-right" action="goto" target="EnvironmentType" />
    <button label="Save" class="glyphicon glyphicon-save" action="put" target="?" />
    <button label="Show All" action="show" target="all" />

    <question group="CustomerName" body="Name" control="ReadOnly" />
    <question group="CustomerCompany" body="Business Name" control="ReadOnly" />
    <question group="CustomerTitle" body="Title" control="Hidden" />
    <question group="CustomerEmailAddress" body="Email Address" control="Hidden" />
    <question group="CustomerPhoneNumber" body="Phone Number" control="Hidden" />
  </page>

  <page slug="EnvironmentType" title="Environment Type" sub-title="Is this a brand new environment, existing storage replacement or a hyper converged integration">
    <button label="Back" class="glyphicon glyphicon-menu-left" action="goto" target="CustomerInfo" />
    <button label="Next" class="glyphicon glyphicon-menu-right" action="goto" target="ScaleIOType" />
    <button label="Save" class="glyphicon glyphicon-save" action="put" target="?" />

    <question group="EnvironmentType" body="New environment" value="New" control="RadioBox" />
    <question group="EnvironmentType" body="Legacy storage replacement" value="Legacy" control="RadioBox">
      <question group="EnvironmentTypeLegacyDetails" body="Details for this environment" control="MultilineTextBox" />
    </question>
    <question group="EnvironmentType" body="Hyper converged integration with existing environment" value="Hyper converged" control="RadioBox">
      <question group="EnvironmentTypeConvergedDetails" body="Details for this environment" control="MultilineTextBox" />
    </question>
  </page>

  <page slug="ScaleIOType" title="Scale IO" sub-title="Do you intend to use ScaleIO standalone or hyper converged?">
    <button label="Back" class="glyphicon glyphicon-menu-left" action="goto" target="EnvironmentType" />
    <button label="Next" class="glyphicon glyphicon-menu-right" action="goto" target="ScaleIOConnect" />
    <button label="Save" class="glyphicon glyphicon-save" action="put" target="?" />

    <question group="ScaleIOType" body="Standalone" value="Standalone" control="RadioBox" />
    <question group="ScaleIOType" body="Hyper Converged" value="Hyper Converged" control="RadioBox" />
  </page>

  <page slug="ScaleIOConnect" title="Scale IO Connection" sub-title="What do you plan to connect to ScaleIO?">
    <button label="Back" class="glyphicon glyphicon-menu-left" action="goto" target="ScaleIOType" />
    <button label="Next" class="glyphicon glyphicon-menu-right" action="goto" target="Virtualized" />
    <button label="Save" class="glyphicon glyphicon-save" action="put" target="?" />
    
    <question group="ScaleIOConnect" body="Standalone servers" value="Standalone" control="CheckBox" />
    <question group="ScaleIOConnect" body="Hypervisor/Virtual servers" value="Virtual" control="CheckBox" />
    <question group="ScaleIOConnect" body="Other" control="CheckOtherBox" />
  </page>

  <page slug="Virtualized" title="Virtualization" sub-title="Are you currently using/planning to use virtualization?">
    <button label="Back" class="glyphicon glyphicon-menu-left" action="goto" target="ScaleIOConnect" />
    <button label="Next" class="glyphicon glyphicon-menu-right" action="goto" target="BareMetalConnectedOS" />
    <button label="Save" class="glyphicon glyphicon-save" action="put" target="?" />
    
    <question group="Virtualized" body="Yes" value="Yes" control="RadioBox">
      <rules>
        <set-if group="EnvironmentType" equals="Hyper converged" />
      </rules>
      <question group="VirtualizedPlatformLabel" body="What platform do they use or prefer" control="Label">
        <question group="VirtualizedPlatform" body="VMware" value="VMware" control="CheckBox" />
        <question group="VirtualizedPlatform" body="KVM" value="KVM" control="CheckBox" />
        <question group="VirtualizedPlatform" body="Xen" value="Xen" control="CheckBox" />
        <question group="VirtualizedPlatform" body="Hyper-V" value="Hyper-V" control="CheckBox" />
        <question group="VirtualizedPlatform" body="Other" control="CheckOtherBox" />
        <question group="VirtualizedPlatform" body="More" control="AddCheckOtherBox" />
      </question>
    </question>
    <question group="Virtualized" body="No" value="N" control="RadioBox">
      <rules>
        <disable-if group="EnvironmentType" equals="Hyper converged" />
        <enable-if-not group="EnvironmentType" equals="Hyper converged" />
        <unset-if group="EnvironmentType" equals="Hyper converged" />
      </rules>
    </question>
  </page>

  <page slug="BareMetalConnectedOS" title="Operating Systems" sub-title="Which operating systems do you plan to run on servers that you plan to connect to ScaleIO?">
    <button label="Back" class="glyphicon glyphicon-menu-left" action="goto" target="Virtualized" />
    <button label="Next" class="glyphicon glyphicon-menu-right" action="goto" target="PerformanceExpectation" />
    <button label="Save" class="glyphicon glyphicon-save" action="put" target="?" />
    
    <question group="BareMetalConnectedOS" body="Windows" value="W" control="CheckBox" />
    <question group="BareMetalConnectedOS" body="Linux" value="L" control="CheckBox" />
    <question group="BareMetalConnectedOS" body="Other" control="CheckOtherBox" />
  </page>

  <page slug="PerformanceExpectation" title="Performance" sub-title="What is the performance/IOPS expectation/behavior for this environment?">
    <button label="Back" class="glyphicon glyphicon-menu-left" action="goto" target="BareMetalConnectedOS" />
    <button label="Next" class="glyphicon glyphicon-menu-right" action="goto" target="EnvironmentApplication" />
    <button label="Save" class="glyphicon glyphicon-save" action="put" target="?" />
    
    <question group="PerformanceExpectation" body="IOPS" control="RadioOtherBox" />
    <question group="PerformanceExpectation" body="Unknown" value="Unk" control="RadioBox">
      <question group="EnvironmentPerformanceTestLabel" body="Do you have the ability to run a diagnostic tool to determine the Environment IOPS?" control="Label">
        <question group="EnvironmentPerformanceTest" body="Yes" value="Yes" control="RadioBox" />
        <question group="EnvironmentPerformanceTest" body="No" value="No" control="RadioBox" />
      </question>
    </question>
  </page>

