  <page slug="Management" title="Management" sub-title="Do you want to manage yourself, or do you want us to manage it?">
    <button label="Back" class="glyphicon glyphicon-menu-left" action="goto" target="StorageGrowth" />
    <button label="Next" class="glyphicon glyphicon-menu-right" action="goto" target="ScaleIOThinProvisioning" />
    <button label="Save" class="glyphicon glyphicon-save" action="put" target="?" />
    
    <question group="ManagedBy" body="No, Self managed" value="SelfManaged" control="RadioBox">
      <question group="SelfManagedSLA" body="What level of support contract do you require?" control="DropDownBox">
        <rules>
          <options>
            <option value="">(Please Choose)</option>
            <option value="4hr">4 hour response</option>
            <option value="24hr">24 hour response</option>
            <option value="1hr">1 hour response</option>
          </options>
        </rules>
      </question>
      <question group="SelfManagedMonitoringLabel" body="Do you require 24-hour monitoring of the environment?" control="Label">
        <question group="SelfManagedMonitoring" body="Yes" value="Yes" control="RadioBox" />
        <question group="SelfManagedMonitoring" body="No" value="No" control="RadioBox" />
      </question>
      <question group="SelfManagedRBACLabel" body="Do you need role based access control to managing ScaleIO Systems?" control="Label">
        <question group="SelfManagedRBAC" body="Yes" value="Yes" control="RadioBox" />
        <question group="SelfManagedRBAC" body="No" value="No" control="RadioBox" />
      </question>
      <question group="SelfManagedAccessIntegrationLabel" body="Do you need integration with existing authentication and authorization system?" control="Label">
        <question group="SelfManagedAccessIntegration" body="Yes" value="Yes" control="RadioBox" />
        <question group="SelfManagedAccessIntegration" body="No" value="No" control="RadioBox" />
      </question>
    </question>
    <question group="ManagedBy" body="Itrica Managed" value="Itrica" control="RadioBox">
      <question group="ManagedSLA" body="What level of support contract do you require?" control="DropDownBox">
        <rules>
          <options>
            <option value="">(Please Choose)</option>
            <option value="4hr">4 hour response</option>
            <option value="24hr">24 hour response</option>
            <option value="1hr">1 hour response</option>
          </options>
        </rules>
      </question>
      <question group="ManagedMonitoringLabel" body="Do you require 24-hour monitoring of the environment?" control="Label">
        <question group="ManagedMonitoring" body="Yes" value="Yes" control="RadioBox" />
        <question group="ManagedMonitoring" body="No" value="No" control="RadioBox" />
