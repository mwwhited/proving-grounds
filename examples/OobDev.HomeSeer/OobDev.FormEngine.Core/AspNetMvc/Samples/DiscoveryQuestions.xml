  <page slug="Backup" title="Backup Option" sub-title="Do you want us to provide you back-up for the environment?">
    <button label="Back" class="glyphicon glyphicon-menu-left" action="goto" target="ScaleIOQoS" />
    <button label="Next" class="glyphicon glyphicon-menu-right" action="goto" target="APIAccess" />
    <button label="Save" class="glyphicon glyphicon-save" action="put" target="?" />
    
