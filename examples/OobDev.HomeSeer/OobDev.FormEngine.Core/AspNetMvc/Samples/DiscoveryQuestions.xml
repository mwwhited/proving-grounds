        <question group="HyperConverged_SQL" body="Yes" value="Yes" control="RadioBox">
          <question group="HyperConverged_SQL_CPU" body="CPU" control="TextBox" />
          <question group="HyperConverged_SQL_Memory" body="Memory" control="TextBox" />
          <question group="HyperConverged_SQL_IOPS" body="IOPS" control="TextBox" />
        </question>
        <question group="HyperConverged_SQL" body="No" value="No" control="RadioBox">
          <question group="NotHyperConverged_SQL_IOPS" body="IOPS" control="TextBox" />
        </question>
      </question>
    </question>

    <question group="EnvironmentApplication" body="Exchange" value="Exchange" control="CheckBox">
      <question group="HyperConvergedLabel_Exchange" body="Is this hyper converged" control="Label">
        <question group="HyperConverged_Exchange" body="Yes" value="Yes" control="RadioBox">
          <question group="HyperConverged_Exchange_CPU" body="CPU" control="TextBox" />
          <question group="HyperConverged_Exchange_Memory" body="Memory" control="TextBox" />
          <question group="HyperConverged_Exchange_IOPS" body="IOPS" control="TextBox" />
        </question>
        <question group="HyperConverged_Exchange" body="No" value="No" control="RadioBox">
          <question group="NotHyperConverged_Exchange_IOPS" body="IOPS" control="TextBox" />
        </question>
      </question>
    </question>

    <question group="EnvironmentApplication" body="Lync" value="Lync" control="CheckBox">
      <question group="HyperConvergedLabel_Lync" body="Is this hyper converged" control="Label">
        <question group="HyperConverged_Lync" body="Yes" value="Yes" control="RadioBox">
          <question group="HyperConverged_Lync_CPU" body="CPU" control="TextBox" />
          <question group="HyperConverged_Lync_Memory" body="Memory" control="TextBox" />
          <question group="HyperConverged_Lync_IOPS" body="IOPS" control="TextBox" />
        </question>
        <question group="HyperConverged_Lync" body="No" value="No" control="RadioBox">
          <question group="NotHyperConverged_Lync_IOPS" body="IOPS" control="TextBox" />
        </question>
      </question>
    </question>

    <question group="EnvironmentApplication" body="Oracle" value="Oracle" control="CheckBox">
      <question group="HyperConvergedLabel_Oracle" body="Is this hyper converged" control="Label">
        <question group="HyperConverged_Oracle" body="Yes" value="Yes" control="RadioBox">
          <question group="HyperConverged_Oracle_CPU" body="CPU" control="TextBox" />
          <question group="HyperConverged_Oracle_Memory" body="Memory" control="TextBox" />
          <question group="HyperConverged_Oracle_IOPS" body="IOPS" control="TextBox" />
        </question>
        <question group="HyperConverged_Oracle" body="No" value="No" control="RadioBox">
          <question group="NotHyperConverged_Oracle_IOPS" body="IOPS" control="TextBox" />
        </question>
      </question>
    </question>

    <question group="EnvironmentApplication" body="SAP" value="SAP" control="CheckBox">
      <question group="HyperConvergedLabel_SAP" body="Is this hyper converged" control="Label">
        <question group="HyperConverged_SAP" body="Yes" value="Yes" control="RadioBox">
          <question group="HyperConverged_SAP_CPU" body="CPU" control="TextBox" />
          <question group="HyperConverged_SAP_Memory" body="Memory" control="TextBox" />
          <question group="HyperConverged_SAP_IOPS" body="IOPS" control="TextBox" />
        </question>
        <question group="HyperConverged_SAP" body="No" value="No" control="RadioBox">
          <question group="NotHyperConverged_SAP_IOPS" body="IOPS" control="TextBox" />
        </question>
      </question>
    </question>

    <question group="EnvironmentApplication" body="Virtualization" value="Virtualization" control="CheckBox">
      <question group="HyperConvergedLabel_Virtualization" body="Is this hyper converged" control="Label">
        <question group="HyperConverged_Virtualization" body="Yes" value="Yes" control="RadioBox">
          <question group="HyperConverged_Virtualization_CPU" body="CPU" control="TextBox" />
          <question group="HyperConverged_Virtualization_Memory" body="Memory" control="TextBox" />
          <question group="HyperConverged_Virtualization_IOPS" body="IOPS" control="TextBox" />
        </question>
        <question group="HyperConverged_Virtualization" body="No" value="No" control="RadioBox">
          <question group="NotHyperConverged_Virtualization_IOPS" body="IOPS" control="TextBox" />
        </question>
      </question>
    </question>

    <question group="EnvironmentApplication" body="Other" control="CheckOtherBox" />
    <question group="EnvironmentApplication" body="None of the above" control="RadioBox" />
  </page>

  <page slug="Network" title="Network Infrastructure">
    <button label="Back" class="glyphicon glyphicon-menu-left" action="goto" target="EnvironmentApplication" />
    <button label="Next" class="glyphicon glyphicon-menu-right" action="goto" target="DataCenter" />
    
    <question group="NetworkManufacturer" body="Who is your network technology manufacturer?" control="DropDownBox">
      <rules>
        <options>
          <option value="">(Please Choose)</option>
          <option value="Cisco">Cisco</option>
          <option value="Arista">Arista</option>
          <option value="HP">HP</option>
          <option value="Mellanox">Mellanox</option>
          <option value="Other">Other</option>
        </options>
      </rules>
    </question>
    <question group="NetworkHardwareModel" body="What is the hardware model of your existing equipment?" control="MultilineTextBox" />
    <question group="NetworkSpeed" body="What is the operating speed?" control="DropDownBox">
      <rules>
        <options>
          <option value="">(Please Choose)</option>
          <option value="1 Gbps">1 Gbps</option>
          <option value="10 Gbps">10 Gbps</option>
          <option value="40 Gbps">40 Gbps</option>
          <option value="56 Gbps">56 Gbps</option>
          <option value="40 GBit infiniband">40 GBit infiniband</option>
          <option value="8GBit Fiber channel">8GBit Fiber channel</option>
          <option value="Other">Other</option>
        </options>
      </rules>
    </question>
    <question group="NetworkAlternativeAllowedLabel" body="Do you have the option to leverage alternative networking equipment?" control="Label">
      <question group="NetworkAlternativeAllowed" body="Yes" value="Yes" control="RadioBox" />
      <question group="NetworkAlternativeAllowed" body="No" value="No" control="RadioBox" />
    </question>
  </page>

  <page slug="DataCenter" title="Data Center" sub-title="Where will this environment be located?">
    <button label="Back" class="glyphicon glyphicon-menu-left" action="goto" target="Network" />
    <button label="Next" class="glyphicon glyphicon-menu-right" action="goto" target="StorageSpace" />
    
    <question group="DataCenterLocation" body="In my own rack" value="On Premise" control="RadioBox" />
    <question group="DataCenterLocation" body="Hosted in a data center" value="Customer Hosted" control="RadioBox">
      <question group="DataCenterRoomAvailableLabel" body="Do you have rack space available?">
        <question group="DataCenterRoomAvailable" body="Yes" value="Yes" control="RadioBox">
          <question group="DataCenterRoomAvailableAmount" body="How much rack space do you have avalible?" control="MultilineTextBox" />
        </question>
        <question group="DataCenterRoomAvailable" body="No" value="No" control="RadioBox" />
      </question>

      <question group="DataCenterElectricityLabel" body="What type of electricity do you have?" control="Label">
        <question group="DataCenterElectricity" body="600VAC" value="600VAC" control="CheckBox" />
        <question group="DataCenterElectricity" body="400VAC" value="400VAC" control="CheckBox" />
        <question group="DataCenterElectricity" body="480VAC" value="480VAC" control="CheckBox" />
        <question group="DataCenterElectricity" body="208VAC" value="208VAC" control="CheckBox" />
        <question group="DataCenterElectricity" body="120VAC" value="120VAC" control="CheckBox" />
      </question>

      <question group="DataCenterRemoteAccessLabel" body="What type of electricity do you have?" control="Label">
        <question group="DataCenterRemoteAccess" body="VPN" value="VPN" control="RadioBox" />
        <question group="DataCenterRemoteAccess" body="Other secure solution" value="Other secure solution" control="RadioBox" />
      </question>
    </question>

    <question group="DataCenterLocation" body="Itrica Provided rack" value="Itrica Hosted" control="RadioBox">

      <question group="FirewallLabel" body="Do you require intrusion protection or firewall?" control="Label">
        <question group="Firewall" body="Yes" value="Yes" control="RadioBox">

          <question group="DedicatedManagementNetworkLabel" body="Do you have a dedicated management network?" control="Label">
            <question group="DedicatedManagementNetwork" body="Yes" value="Yes" control="RadioBox">
              <question group="DedicatedManagementNetworkAddress" body="What is the IP scheme for that network?" control="TextBox" />
              <question group="DedicatedManagementNetworkMask" body="What is the network mask?"  control="DropDownBox">
                <rules>
                  <options>
                    <option value="">(Please Choose)</option>
                    <option value="16">/16</option>
                    <option value="20">/20</option>
                    <option value="22">/22</option>
                    <option value="23">/23</option>
                    <option value="24">/24</option>
                  </options>
                </rules>
              </question>
            </question>
            <question group="DedicatedManagementNetwork" body="No" value="No" control="RadioBox" />
          </question>

          <question group="DedicatedStorageNetworkLabel" body="Do you have a dedicated storage network?" control="Label">
            <question group="DedicatedStorageNetwork" body="Yes" value="Yes" control="RadioBox">
              <question group="DedicatedStorageNetworkAddress" body="What is the IP scheme for that network?" control="TextBox" />
              <question group="DedicatedStorageNetworkMask" body="What is the network mask?" control="DropDownBox">
                <rules>
                  <options>
                    <option value="">(Please Choose)</option>
                    <option value="16">/16</option>
                    <option value="20">/20</option>
                    <option value="22">/22</option>
                    <option value="23">/23</option>
                    <option value="24">/24</option>
                  </options>
                </rules>
              </question>
            </question>
            <question group="DedicatedStorageNetwork" body="No" value="No" control="RadioBox" />
          </question>

        </question>
        <question group="Firewall" body="No" value="No" control="RadioBox" />
      </question>

      <question group="InternetOrCrossConnectLabel" body="Do you require Internet or dedicated connectivity via private circuit?" control="Label">
        <question group="InternetOrCrossConnect" body="Yes" value="Yes" control="RadioBox">
          <question group="DataCenterConnection" body="Existing" value="Existing" control="RadioBox">
            <question group="DataCenterConnectionExistingCarrier" body="What carrier?" control="TextBox" />
          </question>
          <question group="DataCenterConnection" body="New" value="New" control="RadioBox">
            <question group="DataCenterConnectionNewAddress" body="What is the address?" control="MultilineTextBox" />
            <question group="DataCenterConnectionNewSpeed" body="What speed do you need?" control="DropDownBox">
              <rules>
                <options>
                  <option value="">(Please Choose)</option>
                  <option value="1Gbps">1 Gbps</option>
                  <option value="10Gbps">10 Gbps</option>
                  <option value="Other">Other</option>
                </options>
              </rules>
            </question>
          </question>
        </question>
        <question group="InternetOrCrossConnect" body="No" value="No" control="RadioBox" />
      </question>

    </question>
  </page>

  <page slug="StorageSpace" title="Storage Space">
    <button label="Back" class="glyphicon glyphicon-menu-left" action="goto" target="DataCenter" />
    <button label="Next" class="glyphicon glyphicon-menu-right" action="goto" target="StorageGrowth" />
    <button label="Save" class="glyphicon glyphicon-save" action="put" target="?" />
    
    <question group="StorageSpace" body="How much usable disk space do you need?" control="MultilineTextBox" />
